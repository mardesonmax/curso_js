<% if (errors.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-danger">
            <% errors.map(erro => { %>
                <%= erro %><br>
            <% }) %>
        </div>
    </div>
</div>
<% } %>

<% if (success.length > 0) { %>
<div class="row">
    <div class="col">
        <div class="alert alert-success">
            <% success.map(succe => { %>
                <%= succe %><br>
            <% }) %>
        </div>
    </div>
</div>
<% } %>
